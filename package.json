    "webpack:prod": "webpack --config webpack.prod.js",
    "lint": "eslint -c .eslintrc --ext .js,.jsx .",
    "lint:fix": "yarn lint -- --fix",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
